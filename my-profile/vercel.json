{
  "version": 2,
  "rewrites": [
    { 
      "source": "/api/guestbook", 
      "destination": "/backend/api/index.ts" 
    },
    { 
      "source": "/api/guestbook/(.*)", 
      "destination": "/backend/api/index.ts" 
    },
    { 
      "source": "/(.*)", 
      "destination": "/frontend/dist/index.html" 
    }
  ]
}